    "@ai-inbox/tsconfig": "^0.1.0",
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.57.0",
    "jiti": "^1.21.0",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
